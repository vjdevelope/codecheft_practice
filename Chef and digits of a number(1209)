#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    string n;
	    cin>>n;
	    int count1=0,count2=0;
	  
	  for(int i=0;i<n.size();i++)
	  {
	      if(n[i]=='0')
	      count1++;
	      else
	      count2++;
	  }
	  if(count1==1 || count2==1)
	  cout<<"YES"<<endl;
	  else
	  cout<<"NO"<<endl;
	}
	return 0;
}
